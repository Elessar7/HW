using System;

namespace _4_1
{
    class Program
    {
        static void Main(string[] args)
        {
            GetFullName("Афанасьев", "Артём", "Алексеевич");
            GetFullName("Бусыгин", "Дмитрий", "Иванович");
            GetFullName("Пушкарёв", "Владислав", "Павлович");

            static void GetFullName(string firstName, string lastName, string patronymic)
            {
                Console.WriteLine($"{firstName} {lastName} {patronymic}");

            }
        }
    }
}
